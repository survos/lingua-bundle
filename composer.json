{
    "name": "survos/lingua-bundle",
    "description": "Symfony SurvosLinguaBundle",
    "keywords": [
        "symfony-ux",
        "symfony"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/routing": "^7.3",
        "twig/twig": "^3.0",
        "symfony/console": "^7.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Survos\\SurvosLinguaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Survos\\SurvosLinguaBundle\\Tests\\": "tests/"
        }
    }
}