{
    "name": "survos/lingua-bundle",
    "description": "interact with Survos Lingua Server (aka translation server).  Does not translate directly",
    "keywords": [
        "symfony",
        "survos",
        "translation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.3||^8.0",
        "symfony/dependency-injection": "^7.3||^8.0",
        "symfony/http-kernel": "^7.3||^8.0",
        "symfony/framework-bundle": "^7.3||^8.0",
        "symfony/routing": "^7.3",
        "symfony/console": "^7.3||^8.0",
        "symfony/http-client": "^7.3||^8.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^7.3||^8.0"
    },
    "autoload": {
        "psr-4": {
            "Survos\\LinguaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Survos\\LinguaBundle\\Tests\\": "tests/"
        }
    }
}
